{
  "name": "portfolio",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --watch --ssl --ssl-key \\certificates/localhost.key  --ssl-cert \\certificates/localhost.crt --proxy-config proxy-config.json",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docker-run": "docker run --memory 4096m --cpus 2 -v ./app -v /app/node_modules -p 4201:4200 --name portfolio --rm portfolio:dev",
    "docker-run:prod": "docker run -p 3000:80 --rm -it portfolio:prod",
    "docker-restart": "docker restart portfolio",
    "docker": "docker build . -f Dockerfile.dev -t portfolio:dev && docker run -p 3000:80 --rm portfolio:dev",
    "docker-build:prod": "docker run . -t portfolio:prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.5",
    "@angular/cdk": "^11.2.4",
    "@angular/common": "~11.2.5",
    "@angular/compiler": "~11.2.5",
    "@angular/core": "~11.2.5",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~11.2.5",
    "@angular/material": "^11.2.4",
    "@angular/platform-browser": "~11.2.5",
    "@angular/platform-browser-dynamic": "~11.2.5",
    "@angular/router": "~11.2.5",
    "@ng-toolkit/serverless": "^8.1.0",
    "@types/lodash": "^4.14.168",
    "angular-responsive-carousel": "^2.0.2",
    "lodash": "^4.17.21",
    "material": "^0.4.3",
    "ng-particles": "^2.2.4",
    "ngx-toastr": "^13.2.1",
    "rxjs": "~6.6.0",
    "rxjs-compat": "^6.6.6",
    "tslib": "^2.0.0",
    "tsparticles": "^1.19.4",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.3",
    "@angular/cli": "~11.2.4",
    "@angular/compiler-cli": "~11.2.5",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  },
  "proxy": "localhost:8080",
  "/api/*": {
    "changeOrigin": true,
    "target": "localhost:8080",
    "secure": false,
    "pathRewrite": {
      "^/api": ""
    }
  }
}
